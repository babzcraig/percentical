{
  "name": "percentical",
  "description": "An app that shows the percentage of time spent in a year",
  "author": "Babs Craig <babscraig@gmail.com> (https://babscraig.com/)",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-react": "7.12.4",
    "prop-types": "15.6.2",
    "react": "^16.7.0",
